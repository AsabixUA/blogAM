extends _layout

block vars
	- var pageTitle = "Articles page";
	- var pageClass = "articlespage";
	- var pageActive = "articlespage";

block content
	div.main-wrap
	.section-articles
		.filter-icon
			include ../img/ico/filters-ico.svg
		.current-page.mobile-current-page
			a(href='#') Статті
		.container-fluid.container
			.section-articles__inner.row
				include ./modules/_content-filter

				.section-articles__content.row
					.section-articles__date
						p.article-date 14.10.2010
					.section-articles__author
						p.site-text__bold-24 Айдер Муждабаєв
					.section-articles__title
						h2 Секрети психології Путіна
							|<br> ПУТИН СТАВИТ УЛЬТИМАТУМ
					.section-articles__body-wrapper
						.section-articles__body
							.section-articles__text
								p.site-text__large-24 Сегодня в Киеве прошла очередная пресс-конференция президента Украины Владимира Зеленского. Я не буду подробно анализировать ее содержание, поскольку главного вопроса о противостоянии с Россией никто из журналистов президенту так и не задал.
							.section-articles__tags
								.section-articles__tags-content
									span #KRYM
									span #UKRAINA
									span #COVID
									span #PANDEMIA
							.section-articles__reaction
